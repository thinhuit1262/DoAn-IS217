          <Commands>
            <Command>
              <Text>/*
The CALCULATE command controls the aggregation of leaf cells in the cube.
If the CALCULATE command is deleted or modified, the data within the cube is affected.
You should edit this command only if you manually specify how the cube is aggregated.
*/
CALCULATE; 
CREATE DYNAMIC SET CURRENTCUBE.[Query 1]
 AS NONEMPTY(
    [Dim Genre].[Genre Name].[Genre Name].MEMBERS, -- Lấy đích danh các thành viên (không lấy cấp All)
    [Measures].[Total Revenue]                     -- Chỉ giữ lại thằng nào có Doanh thu
) ; 
CREATE DYNAMIC SET CURRENTCUBE.[Game Tren 10 Trieu]
 AS FILTER(
    [Dim Game Info].[Game Name].[Game Name].MEMBERS,
    [Measures].[Total Revenue] &gt; 10000000) ; 
CREATE DYNAMIC SET CURRENTCUBE.[Bo 3 NPT]
 AS GENERATE(
	{[Dim Developer].[Dev Name].&amp;[Valve],
	 [Dim Developer].[Dev Name].&amp;[Ubisoft],
	 [Dim Developer].[Dev Name].&amp;[Capcom]},
	 TOPCOUNT(
	 [Dim Game Info].[Game Name].CHILDREN, 3, [Measures].[Total Revenue])) ; </Text>
            </Command>
          </Commands>
        </MdxScript>
      </MdxScripts>
    </Cube>
